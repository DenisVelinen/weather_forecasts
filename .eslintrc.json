{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": [
      "error",
      {
        "ignoreComments": true
      }
    ]
  },
  "plugins": ["jest"]
}
